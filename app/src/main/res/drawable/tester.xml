<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item >
        <shape
            android:shape="rectangle">
            <solid android:color="#FFF" />
            <corners android:radius="40dp"
                />
        </shape>
    </item>
    <item android:right="2dp"
        android:top="1dp"
        android:left="1dp"
        android:bottom="1dp">
        <shape
            android:shape="rectangle">
            <solid android:color="#FFF"/>
            <corners android:radius="40dp"/>
        </shape>
    </item>
</layer-list>